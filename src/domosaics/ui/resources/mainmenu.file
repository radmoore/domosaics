		<menuitem id="JMenuItem">
			<parameter id="name" value="Documentation"/> 
			<parameter id="actionClass" value="domosaics.ui.actions.ShowManualAction"/> 
			<parameter id="tooltip" value="Open DoMosaics documentation"/> 
		</menuItem>
			
		<menuitem id="JMenuItem">
			<parameter id="name" value="About DoMosaics"/> 
			<parameter id="actionClass" value="domosaics.ui.actions.ShowAboutAction"/> 
