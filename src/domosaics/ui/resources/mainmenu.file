			<menuitem id="JMenuItem">
<!--			<parameter id="name" value="Small Example Dataset"/>
				<parameter id="name" value="Load Example Dataset"/> 
				<parameter id="actionClass" value="domosaics.ui.actions.LoadSmallExampleDataActon"/> 
				<parameter id="tooltip" value="Loads a small example data set"/> 
			</menuItem>
		
<!--			<menuitem id="JMenuItem">
<!--				<parameter id="name" value="Large Example Dataset"/> 
