<menubar>

	<menu id="File">
		
		<menuitem id="JMenuItem">
			<parameter id="name" value="Save Arrangements To File"/> 
			<parameter id="actionClass" value="angstd.ui.views.domainview.actions.SaveXdomFileAction"/> 
			<parameter id="tooltip" value="Saves the arrangements to a file"/> 
		</menuItem>

		<menu id="Image Export">
			<menuitem id="JMenuItem">
				<parameter id="name" value="As PNG"/> 
				<parameter id="actionClass" value="angstd.ui.views.view.actions.ExportViewAsImageAction"/> 
			</menuItem>

			<menuitem id="JMenuItem">
				<parameter id="name" value="As JPG"/> 
				<parameter id="actionClass" value="angstd.ui.views.view.actions.ExportViewAsImageAction"/>
			</menuItem>
		
			<menuitem id="JMenuItem">
				<parameter id="name" value="As BMP"/>
				<parameter id="actionClass" value="angstd.ui.views.view.actions.ExportViewAsImageAction"/> 
			</menuItem>
		
			<menuitem id="JMenuItem">
				<parameter id="name" value="As PDF"/> 
				<parameter id="actionClass" value="angstd.ui.views.view.actions.ExportViewAsImageAction"/> 
			</menuItem>
		
			<menuitem id="JMenuItem">
				<parameter id="name" value="As SVG"/> 
				<parameter id="actionClass" value="angstd.ui.views.view.actions.ExportViewAsImageAction"/> 
			</menuItem>
		</menu>
		
		<menuitem id="JSeparator"/>
		
		<menuitem id="JMenuItem">
			<parameter id="name" value="Close"/> 
			<parameter id="actionClass" value="angstd.ui.views.view.actions.CloseAction"/> 
			<parameter id="tooltip" value="Closes the view"/> 
		</menuItem>
	</menu>

	<menu id="View">
		<menuitem id="JCheckBox">
			<parameter id="name" value="ZoomMode"/> 
			<parameter id="actionClass" value="angstd.ui.views.view.actions.ToggleZoomModeAction"/> 
			<parameter id="tooltip" value="Makes zooming possible using the mouse wheel"/> 
			<parameter id="isSelected" value="false"/>
		</menuItem>
				
		<menuitem id="JSeparator"/>
		
		<menuitem id="JCheckBox">
			<parameter id="name" value="Accession IDs"/> 
			<parameter id="actionClass" value="angstd.ui.views.domainview.actions.ShowIdAction"/> 
			<parameter id="tooltip" value="Label the domains by their native database name accession number"/> 
			<parameter id="isSelected" value="true"/> 
		</menuItem>
		
		<menuitem id="JCheckBox">
			<parameter id="name" value="Domain Names"/> 
			<parameter id="actionClass" value="angstd.ui.views.domainview.actions.ShowAccAction"/> 
			<parameter id="tooltip" value="Label the domains by their native database name"/> 
			<parameter id="isSelected" value="false"/> 
		</menuItem>

		<menuitem id="JSeparator"/>
		
		<menuitem id="JCheckBox">
			<parameter id="name" value="Proportional View"/> 
			<parameter id="actionClass" value="angstd.ui.views.domainview.actions.ProportionalViewAction"/> 
			<parameter id="tooltip" value="Draws all domains by their real size"/> 
			<parameter id="isSelected" value="true"/> 
		</menuItem>
		
		<menuitem id="JCheckBox">
			<parameter id="name" value="Unproportional View"/> 
			<parameter id="actionClass" value="angstd.ui.views.domainview.actions.UnproportionalViewAction"/> 
			<parameter id="tooltip" value="Draws all domains using the same size"/> 
			<parameter id="isSelected" value="false"/> 
		</menuItem>
		
		<menuitem id="JCheckBox">
			<parameter id="name" value="MSA View"/> 
			<parameter id="actionClass" value="angstd.ui.views.domainview.actions.MsaViewAction"/> 
			<parameter id="tooltip" value="Shows the sequence instead of the drawn arrangement"/> 
			<parameter id="isSelected" value="false"/> 
		</menuItem>
		
		<menuitem id="JSeparator"/>
		
		<menuitem id="JCheckBox">
			<parameter id="name" value="FitToScreen"/> 
			<parameter id="actionClass" value="angstd.ui.views.domainview.actions.FitDomainsToScreenAction"/> 
			<parameter id="tooltip" value="Fits the domains into screen size"/> 
			<parameter id="isSelected" value="false"/> 
		</menuItem>
		
		<menuitem id="JCheckBox">
			<parameter id="name" value="Evalue Colorization"/> 
			<parameter id="actionClass" value="angstd.ui.views.domainview.actions.EvalueColorizationAction"/> 
			<parameter id="tooltip" value="Colorizes domains based on their evalues"/> 
			<parameter id="isSelected" value="false"/> 
		</menuItem>
		
		<menuitem id="JCheckBox">
			<parameter id="name" value="Show Shapes"/> 
			<parameter id="actionClass" value="angstd.ui.views.domainview.actions.ShowShapesAction"/> 
			<parameter id="tooltip" value="Shows shapes for domains instead of its name"/> 
			<parameter id="isSelected" value="false"/> 
		</menuItem>
		
		<menuitem id="JCheckBox">
			<parameter id="name" value="Show Notes"/> 
			<parameter id="actionClass" value="angstd.ui.views.domainview.actions.ShowNotesAction"/> 
			<parameter id="tooltip" value="Shows a note marker for arrangements which have a note assigned"/> 
			<parameter id="isSelected" value="true"/> 
		</menuItem>
		
		
		<menuitem id="JSeparator"/>
		
		<menuitem id="JMenuItem">
			<parameter id="name" value="Show Legend"/> 
			<parameter id="actionClass" value="angstd.ui.views.domainview.actions.ShowDomainLegendAction"/> 
			<parameter id="icon" value="angstd/ui/views/domainview/resources/img/tools/legend.png"/>
			<parameter id="tooltip" value="Opens a new window where the domain legend is displayed"/> 
		</menuItem>
		
