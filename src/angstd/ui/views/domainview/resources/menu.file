	<menu id="Select">
	
		<menuitem id="JMenuItem">
			<parameter id="name" value="Select All Domain Arrangements"/> 
			<parameter id="actionClass" value="angstd.ui.views.domainview.actions.SelectAllDomainArrangementsAction"/> 
						<parameter id="icon" value="angstd/ui/views/domainview/resources/img/menu/selectallarrangements.png"/>
			<parameter id="tooltip" value="Selects all domain arrangements at once"/> 
		</menuItem>
		
		<menuitem id="JCheckBox">
			<parameter id="name" value="Select Domain Arrangements"/> 
			<parameter id="actionClass" value="angstd.ui.views.domainview.actions.SelectDomainArrangementsAction"/> 
			<parameter id="icon" value="angstd/ui/views/domainview/resources/img/menu/selectarrangement.png"/>
			<parameter id="tooltip" value="Gives the opportunity to select domain arrangements"/> 
			<parameter id="isSelected" value="true"/> 
		</menuItem>
	
		<menuitem id="JCheckBox">
			<parameter id="name" value="Select Sequences"/> 
			<parameter id="actionClass" value="angstd.ui.views.domainview.actions.SelectSequencesAction"/> 
			<parameter id="icon" value="angstd/ui/views/domainview/resources/img/menu/selectseq.png"/>
			<parameter id="tooltip" value="Gives the opportunity to select parts of sequences"/> 
			<parameter id="isSelected" value="false"/> 
		</menuItem>
		
		<menuitem id="JMenuItem">
			<parameter id="name" value="Reset Shift positions"/> 
			<parameter id="actionClass" value="angstd.ui.views.domainview.actions.ResetShiftAction"/> 
			<parameter id="icon" value="angstd/ui/views/domainview/resources/img/menu/resetshift.png"/>
			<parameter id="tooltip" value="Automatically position all arrangements at the left side"/> 
		</menuItem>
		
		<menuitem id="JMenuItem">
			<parameter id="name" value="Export selection to new view"/> 
			<parameter id="actionClass" value="angstd.ui.views.domainview.actions.CreateViewUsingSelectionAction"/> 
			<parameter id="icon" value="angstd/ui/views/domainview/resources/img/menu/exportselection.png"/>
			<parameter id="tooltip" value="Opens a new domain view containing the selected arrangements"/> 
		</menuItem>
		
		<menuitem id="JMenuItem">
			<parameter id="name" value="Export selection to existing view"/> 
			<parameter id="actionClass" value="angstd.ui.views.domainview.actions.AddSelectionToViewAction"/> 
			<parameter id="icon" value="angstd/ui/views/domainview/resources/img/menu/exportselection.png"/>
			<parameter id="tooltip" value="Opens a new domain view containing the selected arrangements"/> 
		</menuItem>
		
	</menu>
	
